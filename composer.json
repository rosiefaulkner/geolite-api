{
    "name": "rosiefaulkner/api",
    "description": "API in SlimPHP",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2.3"
    },
    "require": {
        "php": "^7.2.5 || ^8",
        "slim/slim": "4.*",
        "slim/http": "^1.2",
        "slim/psr7": "^1.5",
        "selective/basepath": "^2.1",
        "geoip2/geoip2": "^2.12",
        "psr/container": "^1.0",
        "php-di/php-di": "^6.3",
        "slim/twig-view": "^3.0",
        "geocoder-php/geoip2-provider": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Rosiefaulkner\\API\\": "src/"
        }
    },
    "post-install-cmd": [
        "php artisan clear-compiled",
        "chmod -R 777 src/"
    ],
    "authors": [
        {
            "name": "Rosie Faulkner",
            "email": "faulknerproject@gmail.com"
        }
    ]
}
